<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.yangcode.hrm.mapper.PagerMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="cn.yangcode.hrm.domain.Pager">
        <id column="id" property="id" />
        <result column="name" property="name" />
        <result column="alias" property="alias" />
        <result column="type" property="type" />
        <result column="physicalPath" property="physicalPath" />
        <result column="createTime" property="createTime" />
        <result column="site_id" property="siteId" />
        <result column="template_url" property="templateUrl" />
    </resultMap>
    <select id="selectByName" resultType="cn.yangcode.hrm.domain.Pager">
        select id,name,alias,type,physicalPath,createTime,site_id,template_url
        from t_pager
        where name = #{pageName}
    </select>

</mapper>
